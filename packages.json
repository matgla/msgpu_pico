
{
    "dependencies": [
        {
            "link": "https://github.com/raspberrypi/pico-sdk.git",
            "type": "git",
            "version": "1.2.0",
            "directory": "pico-sdk",
            "target": "pico-sdk",
            "options": {
                "update_submodules": true,
                "is_cmake_library": true
            }
        },
        { 
            "link": "https://github.com/matgla/pico-extras.git",
            "type": "git",
            "version": "master",
            "directory": "pico-extras",
            "target": "pico-extras",
            "options": {
                "update_submodules": true,
                "is_cmake_library": true
            }

        },
        {
            "link": "https://github.com/matgla/msgpu_interface.git",
            "type": "git",
            "version": "main",
            "directory": "msgpu_interface",
            "target": "msgpu_interface",
            "options": {
                "cmake_variables": {
                    "DISABLE_TESTS": "ON"
                }
            }
        },
        {
            "link": "https://github.com/matgla/msos_dl.git",
            "type": "git",
            "version": "main",
            "directory": "msos_dl",
            "target": "msos_dl",
            "options": {
                "cmake_variables": {
                    "DISABLE_TESTS": "ON"
                }
            }
        },
        {
            "name": "eul",
            "link": "https://github.com/matgla/EmbeddedUtilityLibraries.git",
            "type": "git",
            "version": "master",
            "directory": "eul",
            "target": "eul",
            "options": {
                "cmake_variables": {
                    "DISABLE_TESTS": "ON",
                    "DISABLE_SANITIZERS": "ON"
                }
            }
        },
        {
            "name": "msgui",
            "link": "https://github.com/matgla/MsGui.git",
            "type": "git",
            "version": "master",
            "directory": "msgui",
            "target": "msgui",
            "options": {
                "cmake_variables": {
                    "DISABLE_TESTS": "ON",
                    "DISABLE_SANITIZERS": "ON"
                }
            }
        },
        {
            "name": "littlefs",
            "link": "https://github.com/littlefs-project/littlefs.git",
            "type": "git",
            "version": "v2.4.0",
            "directory": "littlefs",
            "target": "littlefs",
            "options": {
                "create_library": {
                    "type": "STATIC",
                    "sources_filter": [ "*.c", "*.h"],
                    "sources_directory": "",
                    "include_directories": [""]
                }
            }
        },
        {
            "target": "gtest",
            "version": "master",
            "directory": "gtest",
            "options": { },
            "type": "git",
            "link": "https://github.com/google/googletest.git"
        },
        {
            "target": "sml",
            "version": "v1.1.4",
            "link": "https://github.com/boost-ext/sml.git",
            "type": "git",
            "directory": "sml", 
            "options": {
                "create_library": {
                    "type": "INTERFACE",
                    "sources_filter": ["*.hpp"],
                    "include_directories": ["include"],
                    "sources_directory": "include"
                }
            }
        },
        {
            "target": "cmake_utils",
            "version": "master",
            "link": "https://github.com/matgla/CMakeUtils.git",
            "type": "git", 
            "directory": "cmake_utils",
            "options": {
                "is_cmake_library": true
            }
        }
    ]
}

